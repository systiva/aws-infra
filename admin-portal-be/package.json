{
  "name": "data-mapping-lambda",
  "version": "1.0.0",
  "description": "AWS Lambda backend for Admin portal",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/",
    "test": "mocha ./test/**/*.test.js --timeout=10000 --exit",
    "start": "node ./app.js",
    "local": "set ENV=local&& set AWS_DEFAULT_REGION=us-east-1&& node ./app.js | npx pino-pretty",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:qa": "serverless deploy --stage qa",
    "deploy:uat": "serverless deploy --stage uat",
    "deploy:prod": "serverless deploy --stage prod",
    "logs:dev": "serverless logs -f app --stage dev --tail",
    "logs:prod": "serverless logs -f app --stage prod --tail",
    "remove:dev": "serverless remove --stage dev",
    "remove:prod": "serverless remove --stage prod"
  },
  "author": "Chandan",
  "license": "private",
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.9",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dynamoose": "^4.0.2",
    "express": "^4.21.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "serverless-http": "^3.2.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "chai": "^5.1.2",
    "eslint": "^9.17.0",
    "faker": "^6.6.6",
    "mocha": "^11.0.1",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.4.2",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0"
  }
}
